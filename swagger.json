    "/notices": {
      "get": {
        "tags": ["Notices"],
        "summary": "Get notices",
        "security": [],
        "parameters": [
          {
            "in": "query",
            "name": "keyword",
            "description": "The keyword for searching",
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "category",
            "description": "The category for searching",
            "schema": {
              "type": "string",
              "enum": ["sell", "free", "lost", "found"]
            }
          },
          {
            "in": "query",
            "name": "species",
            "description": "The species for searching",
            "schema": {
              "type": "string",
              "enum": ["dog", "cat", "monkey", "bird", "snake", "turtle", "lizard", "frog", "fish", "ants", "bees", "butterfly", "spider", "scorpion"]
            }
          },
          {
            "in": "query",
            "name": "locationId",
            "description": "The location id for searching",
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "byDate",
            "description": "Sort notices by date",
            "schema": {
              "type": "boolean",
              "default": true
            }
          },
          {
            "in": "query",
            "name": "byPrice",
            "description": "Sort notices by price",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "in": "query",
            "name": "byPopularity",
            "description": "Sort notices by popularity",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "in": "query",
            "name": "page",
            "description": "The number page of the notices",
            "schema": {
              "type": "number",
              "default": 1
            }
          },
          {
            "in": "query",
            "name": "limit",
            "description": "The limit for the notices",
            "schema": {
              "type": "number",
              "default": 6
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetNoticesResponse"
                }
              }
            }
          },
          "404": {
            "description": "Not found",
            "content": {}
          },
          "500": {
            "description": "Server error",
            "content": {}
          }
        }
      }
    },
